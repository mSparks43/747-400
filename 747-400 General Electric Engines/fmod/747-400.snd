	BLEND_DEPTH 0.2
	AABB -3.4 -1.8 -30 3.4 5.9 25
END_SOUND_SPACE

REQUIRES_BANK GEEngines.bank
REQUIRES_BANK Dynamics.bank
REQUIRES_BANK Weather.bank
REQUIRES_BANK Warning.bank

# Sound Mixer ###################################

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /No Int
	EVENT_ALWAYS 
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /No Ext
	EVENT_ALWAYS 
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /Delay
	EVENT_ALWAYS 
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /NoCab
	EVENT_START_COND sim/graphics/view/pilots_head_z < -23.5
	EVENT_END_COND sim/graphics/view/pilots_head_z > -23.1
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /NoCokp
	EVENT_START_COND sim/graphics/view/pilots_head_z > -23.1
	EVENT_END_COND sim/graphics/view/pilots_head_z < -23.5
END_SOUND_ATTACHMENT

